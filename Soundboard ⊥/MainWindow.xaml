<Window x:Class="Soundboard__.MainWindow"
				xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
				xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
				xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
				xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
				xmlns:local="clr-namespace:Soundboard__"
				
				mc:Ignorable="d"
				Title="Pink Gay Soundboard" Height="450" Width="800"
				Closing="Window_Closing" Foreground="White" Background="#FFB90B7A" Loaded="Window_Loaded">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition/>
			<RowDefinition Height="24"/>
		</Grid.RowDefinitions>
		<StackPanel x:Name="StingInputs" Grid.Row="0"/>
		<Button x:Name="StopAllSound" Grid.Row="1" Content="Click Here To Stop All Sound" Foreground="White" Background="Transparent" BorderBrush="#FFFF00D1" FontWeight="Normal" FontSize="16" Padding="0" BorderThickness="2" Click="StopAllSound_Click"/>
	</Grid>
</Window>
